            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  3568957
                ],
                "query": "SELECT latest(`gridgain.dataregion.OffHeapSize`) FROM Metric FACET name WHERE `entity.guid` = 'MzU2ODk1N3xFWFR8U0VSVklDRXwtMTU3OTEyODU3NDQ2NDYwMDQ2MA' SINCE 30 MINUTES AGO TIMESERIES"
              }
            ],
